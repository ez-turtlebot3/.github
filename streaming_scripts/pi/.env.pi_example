# Environment configuration file for EZ-TurtleBot Raspberry Pi
# Copy this file to .env.pi and modify the values as needed
# Do not commit the actual .env.pi file to version control

# AWS IoT Configuration
AWS_IOT_ENDPOINT="xxxxxxxxxxxxxxxxxx.iot.region.amazonaws.com"
DEVICE_CERTIFICATE="/greengrass/v2/device.pem.crt"
DEVICE_PRIVATE_KEY="/greengrass/v2/private.pem.key"
AMAZON_ROOT_CERTIFICATE="/greengrass/v2/AmazonRootCA1.pem"

# Sensor Configuration
MQTT_TOPIC="ez-tb/sensordata/analog"
SENSOR_TYPE="Analog"